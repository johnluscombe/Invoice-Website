<h1>New user</h1><br>
<%= render 'shared/errors', object: @user %>

<%= form_for @user do |form| %>
<%= render partial: 'fields', locals: { f: form } %>

<%= form.submit 'Create new user', :class => "btn btn-success" %>

<button class="btn btn-danger">
  <%= link_to "Cancel", users_path(@user) %>
</button>
<% end %>