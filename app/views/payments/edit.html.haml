%h4 Edit Payment
%br
= render 'shared/errors', object: @payment

= form_for @payment do |form|
  = render partial: 'fields', locals: {f: form }
  %br
  = form.submit 'UPDATE PAYMENT', :class => 'btn btn-success'
  = link_to 'CANCEL', invoice_payments_path(@invoice), class: 'btn btn-danger btn-outline'