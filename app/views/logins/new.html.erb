<h1>Sign In</h1>

<%= form_tag logins_path do %>
    <table class="login">
      <tr>
        <td class="login"><%= text_field_tag :username, params[:username], class: 'form-control', placeholder: 'Username' %></td>
      </tr>
      <tr>
        <td class="login"><%= password_field_tag :password, params[:password], class: 'form-control', placeholder: 'Password' %></td>
        <td class="login"><%= submit_tag 'Log In', class: 'btn btn-default' %></td>
      </tr>
    </table>
<% end %>